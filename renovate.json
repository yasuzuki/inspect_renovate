{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "schedule": ["after 10pm", "before 7am"],
  "labels": ["Dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "enabled": false
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true,
    "labels": ["Security"]
  }
}
